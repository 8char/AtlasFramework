local Driver = AtlasLibrary.ORM.Driver

class MySQL extends Driver
  static queryOrder = {
    Driver.NAME, 
    Driver.TYPE,
    Driver.LENGTH,
    Driver.UNSIGNED,
    Driver.PRIMARY,
    Driver.FOREIGN,
    Driver.INCREMENTS,
    Driver.NULLABLE,
    Driver.ONUPDATE
  }

  __type() return "AtlasLibrary.ORM.MySQL" end
end

AtlasLibrary.ORM.MySQL = MySQL

