 
util.AddNetworkString("AtlasUI.Notification")

function AtlasUI:Notify(ply, str, icon, duration, progressColor, textColor, markup)
  net.Start("AtlasUI.Notification")
    net.WriteTable({
      str = str,
      icon = icon,
      dur = duration,
      progressCol = progressColor,
      textCol = textColor,
      markup = markup
    })
  net.Send(ply)
end
